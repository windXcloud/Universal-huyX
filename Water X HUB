local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Rebirth Champion X", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

-- vaulues
_G.AutoTap = true

--functions

function AutoTap()
While _G.AutoTap == true do 
game:GetService("ReplicatedStorage").Remotes["buy egg"]:InvokeServer({{"Common Egg"},{false}})
wait (0.00001)
end
end
 

-- tabs
local MainTab = Window:MakeTab({
	Name = "Mainüìç",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Tab = Window:MakeTab({
	Name = "Misicüìç",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Tab = Window:MakeTab({
	Name = "Setting ‚öôÔ∏è",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- scripts
MainTab:AddToggle({
	Name = "Auto TapüëÜ",
	Default = false,
	Callback = function(Vaule)
	    _G.AutoTap = Value
      AutoTap()
	end    
})



-- end script
OrionLib:Init()
